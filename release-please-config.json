{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-type": "python",
  "packages": {
    ".": {
      "package-name": "chat-rag-explorer",
      "extra-files": [
        {
          "type": "generic",
          "path": "pyproject.toml",
          "glob": false
        }
      ]
    }
  },
  "changelog-sections": [
    { "type": "feat", "section": "Features" },
